from pygame import *
from random import randint
from time import time as timer

blue = [176, 196, 222]
win_width = 700
win_height = 500
display.set_caption("Ping")
img_back = blue
window = display.set_mode((700, 500))
window.fill(blue)
game = True
FPS=60
clock=time.Clock()
while game:
    for e in event.get():
        if e.type==QUIT:
            game=False
    display.update()
    clock.tick(FPS)
